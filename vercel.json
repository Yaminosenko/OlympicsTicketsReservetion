{
  "version": 2,
  "builds": [
    {
      "src": "frontend/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      },
      "dest": "/frontend/frontend/build/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/frontend/frontend/build/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/frontend/frontend/build/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/frontend/build/index.html"
    }
  ]
}